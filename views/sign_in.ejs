<div class="signing-container">
    <div class="page-title">
        <h1>
            Codeial | <span class="profile-title"><%= title %></span>
        </h1>
        <div class="form-container">
            <form action="/users/create-session" method="POST">
                <div>
                    <input type="email" name="email" placeholder="Email" required>
                </div>
                <div>
                    <input type="password" name="password" placeholder="Password" required>
                </div>
                <div>
                    <button type="submit">Sign In</button>
                </div>
            </form>
        </div>
        <div class="socail-auth" id="google-auth">
            <section>
                    <a href="/users/auth/google"><img src="/images/social-icons/icons8-google-48.png" alt="google-auth"><p>Sign up/in with Google</p></a>
            </section>
        </div>
    </div>
</div>


<script src="/config/passport-local-strategy.js"></script>